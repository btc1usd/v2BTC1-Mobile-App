{
  "cli": {
    "version": ">= 7.8.0"
  },
  "build": {
    "preview": {
      "node": "22.11.0",
      "pnpm": "9.12.0",
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096"
      },
      "android": {
        "buildType": "apk",
        "cache": {
          "key": "preview-cache",
          "cacheDefaultPaths": false,
          "paths": [
            "node_modules"
          ]
        }
      }
    },
    "development": {
      "pnpm": "9.12.0",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "production": {
      "pnpm": "9.12.0",
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
